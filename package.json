{
  "name": "cnn-package-generator",
  "version": "0.2.0",
  "description": "A CLI for generating CNN packages.",
  "main": "lib/cnn-cli.js",
  "bin": {
    "cnn": "./bin/cnn.js"
  },
  "man": [
    "man/man1/cnn.1"
  ],
  "directories": {
    "lib": "./lib",
    "man": "./man",
    "test": "./test"
  },
  "scripts": {
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "generate-manpage": "marked-man src/man/man1/cnn.1.md > $(jq -r .man[] package.json)",
    "test": "eslint *.js .*.js **/*.js **/.*.js && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-package-generator.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-package-generator/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-package-generator",
  "engines": {
    "node": ">=5.5.0"
  },
  "dependencies": {
    "generator-cnn-base": "0.1.8",
    "generator-cnn-cli": "0.1.1",
    "generator-cnn-generator": "0.1.3",
    "init-package-json": "1.9.3",
    "minimist": "1.2.0",
    "npm-package-arg": "4.1.0",
    "validate-npm-package-name": "2.2.2",
    "yeoman-environment": "1.5.2",
    "yeoman-generator": "0.22.5"
  },
  "devDependencies": {
    "changelog-maker": "2.2.0",
    "esdoc": "0.4.5",
    "eslint": "2.1.0",
    "jq-cli-wrapper": "0.3.0",
    "jsonlint": "1.6.2",
    "marked-man": "0.1.5",
    "npm-check-updates": "2.5.8"
  },
  "os": [
    "!win32"
  ],
  "keywords": [
    "cnn",
    "cnnlabs",
    "cli",
    "cnn-package-generator"
  ],
  "license": "MIT"
}
